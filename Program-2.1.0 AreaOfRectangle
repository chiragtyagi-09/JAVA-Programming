import java.util.*;
public class Hello{
    public static void main(String[] args){
        Scanner sc= new Scanner(System.in);

    System.out.print("Enter length : ");
    int a= sc.nextInt();
    System.out.print("Enter breadth : ");
    int b= sc.nextInt();

    int area=a*b;
    System.out.println("Area of rectangle = "+area);
    }
}
