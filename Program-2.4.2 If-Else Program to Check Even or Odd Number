import java.util.*;
public class Hello{
    public static void main(String[] args){
        Scanner sc= new Scanner(System.in);

        System.out.print("Enter number to check whether it's even or odd : ");
        int num= sc.nextInt();

        if( num%2 == 0 ) {
            System.out.println("Even Number");
        }
       else{
            System.out.println("Odd Number");
        }
    }
}
