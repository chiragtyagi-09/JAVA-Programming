import java.util.*;
public class Hello{
    public static void main(String[] args){
        Scanner sc= new Scanner(System.in);

        int i;

        System.out.print("Enter how many times you want to print Hello World : ");
        int n= sc.nextInt();

        for (i=1;i<=n;i++)
        {
            System.out.println("Hello World");
        }

    }
}
