import java.util.*;
public class Hello {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);


        System.out.print("Enter a number : ");
        int n =sc.nextInt();
        int m=n;
        int rev=0,r;

        while(n>0)
        {
            r=n%10;
            rev=rev*10+r;
            n/=10;
        }

        if (rev==m)
            System.out.print("Palindrome");
        else
            System.out.print("Not a Palindrome");
     
    }
}
