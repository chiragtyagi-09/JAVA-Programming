import java.util.*;
public class Hello{
    public static void main(String[] args){
        Scanner sc= new Scanner(System.in);

        System.out.print("Enter age to check voting eligibility : ");
        int age= sc.nextInt();

        if( age>=18 && age<130 ) {
            System.out.println("You can vote ");
        }
       else{
            System.out.println("You are not eligible to vote ");
        }
    }
}
