import java.util.*;
public class Hello{
    public static void main(String[] args){
        Scanner sc= new Scanner(System.in);

        int i;

        System.out.print("Enter a number to print its table  : ");
        int num= sc.nextInt();

        for (i=1;i<=10;++i)
        {
            System.out.printf("%d * %d = %d \n",num,i,num*i);
        }

    }
}
