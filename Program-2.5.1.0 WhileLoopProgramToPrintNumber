import java.util.*;
public class Hello {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int i=1;

        System.out.print("Enter how many numbers you want to print : ");
        int n = sc.nextInt();

            while( i<=n )
            {
                System.out.print(" "+i);
                ++i;
            }
            
        }
    }
